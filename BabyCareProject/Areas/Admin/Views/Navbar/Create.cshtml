@using BabyCareProject.Dtos.NavbarDtos
@model CreateNavbarDto
@{
    ViewData["Title"] = "Create";
}
<h1>Yeni Navbar Ekle</h1>

<form method="post" asp-area="Admin">
    <label>Konum</label>
    <input type="text" asp-for="Location" class="form-control" />
